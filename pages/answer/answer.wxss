/**index.wxss**/

.container{
  height: 1500rpx;
  background: #F7F7F7;
}
.container .search{
  width: 100%;
  height: 70rpx;
  background: #fff;
  margin: 0;
}
.search-placeholder{
  color: #A4A4A4;
}
.container .search .search-left{
    margin: 20rpx;
    background: #F7F7F7;
    border-radius: 30rpx;
    display: flex;
    line-height: 100rpx;
}
.container .search .search-left image{
    width: 35rpx;
    height: 35rpx;
    margin: 15rpx;
}
.container .search .search-left input{
    font-size: 24rpx;
    margin-left: 0;
    margin-top: 10rpx;

}
.container{
  padding: 0;
  font-size: 18rpx;
  color: #000;
}
.midView{
  background-color: #F7F7F7;
  display: flex;
  flex-direction:row;
  justify-content:space-around;
}
.itemClass{
  margin: 20rpx;
  width: 100%;
  height: 250rpx;
  background-color: #fff;
  display: flex;
  justify-content: center; 
  flex-direction:column;
  align-items: center;
}
.textStyle{
  width:100%;
  font-size: 32rpx;
  margin-top: 30rpx;
  text-align: center;
  color: #333333;
}
.imageStyle{
  margin-top: 20rpx;
   width:60rpx; 
   height: 60rpx;
}
.imageStyleOther{
   margin-top: 20rpx;
   width:40rpx; 
   height: 40rpx;
}
.smallStyle{
   margin-top:18rpx;
   margin-left:12rpx;
   font-size: 32rpx;


   display: -webkit-box;    
word-break: break-all;
text-overflow: ellipsis; 
overflow: hidden;           
-webkit-box-orient: vertical;    
-webkit-line-clamp: 1;    
}


.textStyleOther{
  width:100%;
  font-size: 28rpx;
  margin-top: 30rpx;
  text-align: center;
  color: #999999;
}
.subMitBtn{
    margin-top: 100rpx;
    width: 90%;
    background:#FD999B;
    border:0;
    color: white;
  }
